<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane
	fx:id="AP"
	prefHeight="600.0"
	prefWidth="800.0"
	xmlns="http://javafx.com/javafx/18"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.view.F02_client_controller">
	<children>
		<Pane
			fx:id="P"
			prefHeight="600.0"
			prefWidth="800.0"
			AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<children>
				<TableView
					fx:id="TV"
					layoutX="62.0"
					layoutY="48.0"
					prefHeight="400.0"
					prefWidth="677.0">
					<columns>
						<TableColumn
							fx:id="col_id_abonne"
							prefWidth="75.0"
							text="id_abonne" />
						<TableColumn
							fx:id="col_prenom"
							prefWidth="130.0"
							text="prenom" />
						<TableColumn
							fx:id="col_nom"
							prefWidth="130.0"
							text="nom" />
						<TableColumn
							fx:id="col_telephone"
							prefWidth="130.0"
							text="telephone" />
						<TableColumn
							fx:id="col_mail"
							prefWidth="210.0"
							text="mail" />
					</columns>
				</TableView>
				<Button
					fx:id="btn_quitter_client"
					layoutX="62.0"
					layoutY="512.0"
					mnemonicParsing="false"
					onAction="#quitter_client"
					text="Quitter" />
				<Button
					fx:id="btn_creer_client"
					layoutX="203.0"
					layoutY="512.0"
					mnemonicParsing="false"
					onAction="#creer_client"
					text="Creer" />
				<Button
					fx:id="btn_editer_client"
					layoutX="339.0"
					layoutY="512.0"
					mnemonicParsing="false"
					onAction="#editer_client"
					text="Editer" />
				<Button
					fx:id="btn_effacer_client"
					layoutX="472.0"
					layoutY="512.0"
					mnemonicParsing="false"
					onAction="#effacer_client"
					text="Effacer" />
			</children>
		</Pane>
	</children>
</AnchorPane>
