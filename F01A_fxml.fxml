<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane
	fx:id="AP"
	prefHeight="200.0"
	prefWidth="400.0"
	xmlns="http://javafx.com/javafx/18"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.view.F01A_controller">
	<children>
		<Pane
			fx:id="P"
			prefHeight="200.0"
			prefWidth="400.0"
			AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<children>
				<Label
					fx:id="lbl_presente_ID"
					layoutX="17.0"
					layoutY="31.0"
					text="ID: (non editable)" />
				<Label
					fx:id="lbl_presente_AUTEUR"
					layoutX="17.0"
					layoutY="62.0"
					text="Auteur:" />
				<Label
					fx:id="lbl_presente_TITRE"
					layoutX="17.0"
					layoutY="92.0"
					text="Titre:" />
				<TextField
					fx:id="txtF_contient_ID"
					editable="false"
					layoutX="181.0"
					layoutY="27.0" />
				<TextField
					fx:id="txtF_contient_AUTEUR"
					layoutX="181.0"
					layoutY="58.0" />
				<TextField
					fx:id="txtF_contient_TITRE"
					layoutX="181.0"
					layoutY="88.0" />
				<Button
					fx:id="btn_inserer"
					layoutX="181.0"
					layoutY="149.0"
					mnemonicParsing="false"
					onAction="#insererLivre"
					text="Inserer" />
				<Button
					fx:id="btn_annuler"
					layoutX="289.0"
					layoutY="149.0"
					mnemonicParsing="false"
					onAction="#annuler"
					text="Annuler" />
			</children>
		</Pane>
	</children>
</AnchorPane>
